@using Practica.Models
@model IEnumerable<Practica.Models.Etiqueta>

@{
    ViewBag.Title = "Etiquetas";
    var almac = ViewBag.almacenes as List<Almacen>;
}



<div class="col-lg-offset-2 col-lg-4">
    <h2>@ViewData["TituloEtiqueta"]</h2>
    <div class="col-lg-12">
        @foreach (var item in Model)
        {
            <ul>
                <li>
                    @Html.DisplayFor(o => item.texto)
                    <ul>

                        @foreach (var producto in item.Producto)
                        {
                            <li>
                                @Html.DisplayFor(o => producto.nombre)
                            </li>
                        }

                    </ul>
                </li>
            </ul>
        }
    </div>
</div>

<div class="col-lg-4">
    <h2>@ViewData["TituloAlmacen"]</h2>
    <div class="col-lg-12">
        @foreach (var alm in almac)
        {
            <ul>
                <li>
                    @Html.DisplayFor(o => alm.ciudad)
                </li>
            </ul>
        }
    </div>
</div>

